<?xml version="1.0" encoding="UTF-8"?>

<?import com.gluonhq.charm.glisten.mvc.View?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>


<View fx:id="cajaView" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dysconcsa.pancasan.views.caja.CajasPresenter">
   <center>
      <TableView fx:id="tableCajas" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="columnCodigo" prefWidth="75.0" text="Codigo" />
          <TableColumn fx:id="columnDescripcion" prefWidth="75.0" text="Descripcion" />
            <TableColumn fx:id="columnUbicacion" prefWidth="75.0" text="Ubicacion" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</View>
